{
  "name": "@osjs/standard-dark-theme",
  "version": "4.0.9",
  "description": "OS.js v3 Standard Dark Theme",
  "files": [
    "dist/",
    "metadata.json"
  ],
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "eslint": "eslint index.js",
    "stylelint": "stylelint index.scss src/**/*.scss",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run test && rm -rf ./dist/* && NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/os-js/osjs-standard-dark-theme.git"
  },
  "keywords": [
    "osjs"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@osjs/eslint-config": "^1.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.14.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/os-js/osjs-standard-dark-theme/issues"
  },
  "homepage": "https://github.com/os-js/osjs-standard-dark-theme#readme",
  "osjs": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "at-rule-no-unknown": [true, {
        "ignoreAtRules": ["extend", "mixin", "include"]
      }]
    },
    "extends": "stylelint-config-standard"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
